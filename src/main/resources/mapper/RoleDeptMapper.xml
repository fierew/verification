<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.weigh.verification.dao.rbac.RoleResourceDao">
    <insert id="addAll">
        INSERT INTO `rbac_role_dept` (`role_id`, `dept_id`, `create_time`, `update_time`)
        VALUES
        <foreach collection="list" item="roleResource" separator=",">
            (#{roleResource.roleId}, #{roleResource.resourceId}, #{roleResource.createTime}, #{roleResource.updateTime})
        </foreach>
    </insert>
</mapper>